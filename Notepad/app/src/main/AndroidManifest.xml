<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.ifro.vilhena.ads.notepad">
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_pencil_detext_9967"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_pencil_detext_9967"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ListarNotasActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CadastrarNotaActivity"
            android:parentActivityName=".ListarNotasActivity" />
        <activity android:name=".AlterarNotaActivity"></activity>
    </application>
</manifest>

